{
  "servers": {
    "microsoft-docs": {
      "url": "https://learn.microsoft.com/api/mcp"
    },
    "time": {
      "command": "docker",
      "args": [
        "run",
        "--name",
        "mcp-time-server",
        "-i",
        "--rm",
        "mcp/time"
      ]
    },
    "echo": {
      "command": "docker",
      "args": [
        "run",
        "--name",
        "mcp-echo-server",
        "-p",
        "8081:80",
        "-i",
        "--rm",
        "ealen/echo-server:latest"
      ]
    }
  }
}